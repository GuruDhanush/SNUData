<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="SNUData2.Views.NewItemPage"
		Title="New Item">
	<ContentPage.ToolbarItems>
		<ToolbarItem Text="Save" />
	</ContentPage.ToolbarItems>
	<ContentPage.Content>
		<StackLayout Spacing="20" Padding="15">
			
            <DatePicker x:Name="StartDate" DateSelected="StartDate_DateSelected"  />
            <DatePicker x:Name="EndDate" DateSelected="EndDate_DateSelected" />

            <StackLayout Orientation="Horizontal" >
                <Button x:Name="Login" Text="Login" Clicked="Login_Clicked"/>
                <Button x:Name="SubmitButton" Text="Submit" Clicked="SubmitButton_Clicked"/>
            </StackLayout>
            
      <WebView x:Name="Data_webView" HeightRequest="700" WidthRequest="300" />
        </StackLayout>
	</ContentPage.Content>
</ContentPage>
